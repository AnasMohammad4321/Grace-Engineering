<h1>Inventory</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Description</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>
  <tbody>
    <% @inventories.each do |inventory| %>
      <tr>
        <td><%= inventory.id %></td>
        <td><%= inventory.name %></td>
        <td><%= inventory.description %></td>Q
        <td><%= inventory.quantity %></td>
        <td><%= inventory.created_at %></td>
        <td><%= inventory.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
